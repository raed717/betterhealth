<script>
// Get the elements
const appointmentButton = document.getElementById('appointmentButton');
const registerPopup = document.getElementById('registerPopup');
const closeButtons = document.querySelectorAll('.close');

// Show the registration popup when the appointment button is clicked
appointmentButton.addEventListener('click', function(event) {
    event.preventDefault(); // Prevent the default link behavior
    registerPopup.style.display = 'flex';
});

// Hide the popup when any close button is clicked
closeButtons.forEach(button => {
    button.addEventListener('click', function() {
        registerPopup.style.display = 'none';
    });
});

// Hide the popup when clicking outside of it
window.addEventListener('click', function(event) {
    if (event.target === registerPopup) {
        registerPopup.style.display = 'none';
    }
});
// Handle form submission
document.getElementById('registerForm').addEventListener('submit', function(event) {
    event.preventDefault();

    // Get form values
    const fullName = document.getElementById('fullName').value;
    const email = document.getElementById('email').value;
    const phone = document.getElementById('phone').value;
    const password = document.getElementById('password').value;
    const repeatPassword = document.getElementById('repeatPassword').value;

    // Clear previous error messages
    const passwordError = document.getElementById('passwordError');
    if (passwordError) {
        passwordError.remove();
    }

    // Validate password match
    if (password !== repeatPassword) {
        // Show an error message under the repeatPassword field
        const errorMessage = document.createElement('p');
        errorMessage.id = 'passwordError';
        errorMessage.style.color = 'red';
        errorMessage.textContent = "Passwords do not match. Please try again.";

        // Insert error message after the repeatPassword input
        document.getElementById('repeatPassword').after(errorMessage);

        // Focus on the password fields
        document.getElementById('password').focus();
        return; // Stop form submission
    }

    // If password match, you can proceed with form submission logic (e.g., sending data to the server)

    // Close the popup after submission
    registerPopup.style.display = 'none';
});

</script>
